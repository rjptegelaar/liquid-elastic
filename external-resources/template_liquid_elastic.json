{
	"template":"liquid",
	"mappings":{
		"liquid_elastic":{
			"properties":{
				"CorrelationID":{
					"type":"string",
					"index":"not_analyzed"
				},
				"Header":{
					"properties":{
						"key":{
							"type":"string",
							"index":"not_analyzed"
						},
						"messageid":{
							"type":"string",
							"index":"not_analyzed"
						},
						"value":{
							"type":"string",
							"index":"not_analyzed"
						}
						
					}
					
				},
				"ID":{
					"type":"string",
					"index":"not_analyzed"
				},
				"Location":{
					"type":"string",
					"index":"not_analyzed"
				},
				"LocationArray":{
					"type":"string",
					"index":"not_analyzed"
				},
				"NumberOfTimesCrawled":{
					"type":"long"
				},
				"Order":{
					"type":"long"
				},
				"ParentID":{
					"type":"string",
					"index":"not_analyzed"
				},
				"Part":{
					"properties":{
						"Content":{
							"type":"string",
							"index":"not_analyzed"
						},
						"index":{
							"type":"long"
						},
						"label":{
							"type":"string",
							"index":"not_analyzed"
						},
						"messageid":{
							"type":"string",
							"index":"not_analyzed"
						}
						
					}
					
				},
				"SnapshotTime":{
					"type":"date",
					"format":"yyyy-MM-dd'T'HH:mm:ss.SSSZ"
				},
				"SnapshotTimeMillis":{
					"type":"long"
				}
				
			}
			
		}
		
	}
}